<% include partials/header %>
 <div class="container">
     <header class="jumbotron">
         <div class="container">
             <h1>Welcome To HelpPro</h1>
             <p>Effiecent Resourse</p>
             <p>
                <a class="btn btn-primary btn-large" href="new">Add New Supply</a>
             </p>
         </div>
     </header>
     
     <div class="row text-center" style="display:flex; flex-wrap: wrap;">
        <% supplies.forEach(function(campground){ %>
            <% if (campground.Amount == 0) { %>
                <%    return;  %>
                <%  } %>
            <div class="col-md-3 col-sm-6">
                <div class="thumbnail">
                   <div class="caption">
                       <h3><%= campground.name %></h3>
                       <h4><%= campground.Area %>: <%= campground.Amount %></h4>
                       <% var a="/"+campground.name +"/" + campground.Area +"/"+ campground.Amount + "/" + campground.Contact +"/use" ; %>
                        
                       <a class="btn btn-primary " href=<%= a %>>Use</a>
                   </div>
                </div>
            </div>
        <% }); %>
    </div>
</div>

<% include partials/footer %>
